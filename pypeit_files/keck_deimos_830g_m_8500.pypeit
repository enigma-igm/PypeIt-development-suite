# Multi-slit for DEIMOS

[rdx]
    spectrograph = keck_deimos
    sortroot = keck_deimos_setup_A
    detnum = 3,7
[baseprocess]
   use_biasimage=False
[reduce]
   [[findobj]]
         sig_thresh = 100.  #  This is purposely set to a high value to miss some obvious targets, so that we can check if PypeIt finds them using the slitmask design info.
   [[extraction]]
      use_2dmodel_mask = False
      boxcar_radius = 1.5  # arcsec
   [[skysub]]
      mask_by_boxcar = True
      no_local_sky = True
# For testing pypeit_deimos_flexure   
[flexure]
   spec_method = skip 
[calibrations]
   [[wavelengths]]
      refframe= observed


# Setup
setup read
 Setup A
 --:
   dichroic: None
   disperser: {angle: 8500.0078125, name: 830G}
   slit: {decker: CFHQS1B, slitlen: None, slitwid: None}
 '01': {binning: '1,1', det: 1, namp: 1}
 '02': {binning: '1,1', det: 2, namp: 1}
 '03': {binning: '1,1', det: 3, namp: 1}
 '04': {binning: '1,1', det: 4, namp: 1}
 '05': {binning: '1,1', det: 5, namp: 1}
